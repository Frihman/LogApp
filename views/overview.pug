extends layout

block content
    link(href='/stylesheets/overviewStyle.css' rel='stylesheet')
    div#mainContent
        h2 #{title}
        select(name='moodFilter' id='moodFilter')
          option(value='all') All
          option(value='😃') 😃
          option(value='😐') 😐
          option(value='🙁') 🙁
        hr
        ul#entries

        script(src='/javascripts/overview.js')